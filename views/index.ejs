<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <title><%= title %></title>
    <link rel="stylesheet" href="/css/fonts.css">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/jquery.notifyBar.css">
    <% for(var i=0; i<styles.length; i++) {%>
    <link rel="stylesheet" href="<%= styles[i] %>"><% } %>
  </head>
  <script>
    var options = <%- JSON.stringify(options) %>;
  </script>
  <script type="text/javascript" src="/socket.io/socket.io.js"></script>
  <script type="text/javascript" src="/components/jquery/jquery.js"></script>
  <script type="text/javascript" src="/js/jquery.fullscreen-min.js"></script>
  <script type="text/javascript" src="/js/polyfills.js"></script>
  <script type="text/javascript" src="/js/cockpit.js"></script>
  <script type="text/javascript" src="/js/vr.js"></script>
  <script type="text/javascript" src="/js/jquery.notifyBar.js"></script>
  <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyClGKomUcD0dxd4GPbTvhQLrjUypFIg0cI"></script>
  <script type="text/javascript" src="/js/map.js"></script>
  <% for(var i=0; i<scripts.length; i++) {%>
  <script type="text/javascript" src="<%= scripts[i] %>"></script><% } %>

  <body>
    <div class="main-container">
      <div class="main wrapper clearfix">
        <div id="glasspane">
          <div id="cockpit">
            <div id="hud-overlay">
              <div class="bottom" id="bottom-bar">
                <img src="/img/Wifi Bar-01.png" id="wifi-bar">
                <img src="/img/Time Bar-01.png" id="time-bar">
                <img src="/img/Location Bar-01.png" id="location-bar">
              </div>
              <div class="bottom" id="map-canvas">
                <!-- <img src="/img/Map Marker-01.png" id="location indicator">! -->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script>
      var cockpit = new Cockpit();
    </script>
  </body>
</html>
